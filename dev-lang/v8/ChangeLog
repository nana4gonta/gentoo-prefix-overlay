  -files/v8-upstream-bug-773-r0.patch, -files/v8-upstream-bug-1016-r0.patch,
  -files/v8-upstream-bug-1326-r0.patch, v8-9999.ebuild,
  -files/v8-gentoo-bug-349794-r0.patch:
